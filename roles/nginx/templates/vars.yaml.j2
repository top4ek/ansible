nginx_proxies:
{% for proxy in nginx_proxies %}
  - domain: {{ proxy.domain }}
    zone: {{ proxy.zone }}
    dest: {{ proxy.dest }}
    enabled: {{ proxy.enabled }}
{% endfor %}

acme_email: {{ acme_email }}
acme_common_name: {{ acme_common_name }}

dns_cloudflare_email: {{ dns_cloudflare_email }}
dns_cloudflare_api_token: {{ dns_cloudflare_api_token }}

nginx_zones:
  -  {{ acme_common_name }}
