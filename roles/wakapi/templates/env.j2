ENVIRONMENT=production
WAKAPI_AGGREGATION_TIME=0 15 2 * * *
WAKAPI_REPORT_TIME_WEEKLY=0 0 18 * * 5
WAKAPI_IMPORT_BATCH_SIZE=50
WAKAPI_INACTIVE_DAYS=30
WAKAPI_HEARTBEAT_MAX_AGE=4320h
WAKAPI_LISTEN_IPV4=0.0.0.0
WAKAPI_LISTEN_IPV6=
WAKAPI_TIMEOUT_SEC=30
WAKAPI_BASE_PATH=/
WAKAPI_INSECURE_COOKIES=false
WAKAPI_COOKIE_MAX_AGE=172800
WAKAPI_ALLOW_SIGNUP=false
WAKAPI_EXPOSE_METRICS=false
WAKAPI_DB_TYPE=postgres
WAKAPI_DB_MAX_CONNECTIONS=2
WAKAPI_DB_SSL=false
WAKAPI_DB_AUTOMIGRATE_FAIL_SILENTLY=false
#WAKAPI_MAIL_PROVIDER=smtp
#WAKAPI_MAIL_SMTP_HOST={{ wakapi.email.host }}
#WAKAPI_MAIL_SMTP_PORT={{ wakapi.email.port }}
#WAKAPI_MAIL_SMTP_USER={{ wakapi.email.user }}
#WAKAPI_MAIL_SMTP_PASS={{ wakapi.email.password }}
#WAKAPI_MAIL_SMTP_TLS={{ wakapi.email.tls }}
WAKAPI_SENTRY_TRACING=false
WAKAPI_SENTRY_SAMPLE_RATE=0.75
WAKAPI_SENTRY_SAMPLE_RATE_HEARTBEATS=01
WAKAPI_QUICK_START=false
WAKAPI_SENTRY_DSN={{ wakapi.sentry_dsn }}
WAKAPI_PASSWORD_SALT={{ wakapi.password_salt }}
WAKAPI_DB_HOST={{ wakapi.postgres.host }}
WAKAPI_DB_PORT={{ wakapi.postgres.port }}
WAKAPI_DB_USER={{ wakapi.postgres.user }}
WAKAPI_DB_PASSWORD={{ wakapi.postgres.password }}
WAKAPI_DB_NAME={{ wakapi.postgres.name }}
WAKAPI_MAIL_ENABLED={{ wakapi.email.enabled }}
WAKAPI_PORT=3000
